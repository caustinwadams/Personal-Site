Rails.application.routes.draw do

  get 'blogs/new'
  get 'blogs/index'
  get 'blogs/show'
  get 'blog/new'
  get 'blog/index'
  get 'blog/show'
  get 'blog/destroy'
  get 'static_pages/home'
  get 'static_pages/resume'
  root 'static_pages#home'
  get '/resume', to: 'static_pages#resume'
  get '/blog',   to: 'blog#index'
end
